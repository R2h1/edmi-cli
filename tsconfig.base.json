{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "esModuleInterop": true,
    "removeComments": true,
    "sourceMap": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "newLine": "LF",
    "declaration": true,
    "noImplicitAny": true,
    "moduleResolution": "nodenext",
    "noImplicitOverride": true,
    "noUnusedLocals": true,
    "composite": true,
  },
}
